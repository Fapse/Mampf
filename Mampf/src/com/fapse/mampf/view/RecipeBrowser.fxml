<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fapse.mampf.view.RecipeBrowserController">
   <children>
      <VBox>
         <children>
            <Label fx:id="recipeNameLabel" alignment="CENTER" minWidth="300.0" prefHeight="50.0" prefWidth="300.0" text="Label" underline="true">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Label fx:id="condimentsLabel" alignment="TOP_LEFT" minWidth="300.0" prefWidth="300.0" text="Label">
               <VBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </VBox.margin></Label>
            <Label fx:id="recipeTextLabel" alignment="TOP_LEFT" minWidth="300.0" prefWidth="300.0" text="Label">
               <VBox.margin>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </VBox.margin></Label>
            <HBox alignment="CENTER" minWidth="300.0" prefHeight="50.0" prefWidth="300.0">
               <children>
                  <Button fx:id="okButton" mnemonicParsing="false" onAction="#handleOK" text="OK" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
