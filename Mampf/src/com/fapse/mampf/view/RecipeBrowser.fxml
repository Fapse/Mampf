<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fapse.mampf.view.RecipeBrowserController">
   <children>
      <VBox prefWidth="300.0">
         <children>
            <Label fx:id="recipeNameLabel" alignment="CENTER" minWidth="300.0" prefHeight="50.0" prefWidth="300.0" text="Label" underline="true">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <ScrollPane maxHeight="150.0" minHeight="150.0" prefHeight="150.0">
               <content>
                  <VBox maxWidth="270.0" spacing="10.0">
                     <children>
                        <Label fx:id="condimentsLabel" alignment="TOP_LEFT" maxWidth="270.0" minWidth="270.0" prefWidth="270.0" text="Label" />
                        <Label fx:id="recipeTextLabel" alignment="TOP_LEFT" maxWidth="270.0" minWidth="270.0" prefWidth="270.0" text="Label" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
            <HBox alignment="CENTER" minWidth="300.0" prefHeight="50.0" prefWidth="300.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleLastRecipe" text="&lt;&lt;">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="okButton" mnemonicParsing="false" onAction="#handleOK" text="OK">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#handleNextRecipe" text="&gt;&gt;">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
